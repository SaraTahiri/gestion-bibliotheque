package model;

public class Utilisateur {
    private int id;
    private String nom;
    private String email;
    private String role; // admin, membre, biblioth√©caire

    // Constructeur
    public Utilisateur(int id, String nom, String email, String role) {
        this.id = id;
        this.nom = nom;
        this.email = email;
        this.role = role;
    }

    public Utilisateur(int id, String nom) {
		// TODO Auto-generated constructor stub
	}

	// Getters
    public String getNom() {
        return nom;
    }
    public int getId() {
        return id;
    }
}
